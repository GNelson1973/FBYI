<main class="container col-md-10 col-md-offset-3">
<div class="col-xs-8">
<h1>Yours</h1>
<br>
<% @posts.each do |post| %>
  <h4><%= post.content %></h4>
  <div class="thumbnail" ><%= image_tag post.image_url.to_s %> </div>
<% end %>

<br>
<h1>Others</h1>
<br>
<% @likes.each do |like| %>
  <% if like.post.user_id != @user.id %>
    <h4><%= like.post.content %></h4>
    <div class="thumbnail" ><%= image_tag like.post.image_url.to_s %> </div>
  <% end %>
<% end %>
</div>
</main>
